{
  "name": "serverless-azure-functions-generate-pdf",
  "version": "1.0.0",
  "description": "Generate PDFs in a serverless way with Azure Functions and pdf-lib",
  "main": "index.js",
  "scripts": {
    "start": "sls offline",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "deploy": "sls deploy",
    "deploy:storage": "sh ./scripts/setup.sh",
    "upload:fonts": "sh ./scripts/upload-fonts.sh",
    "teardown": "sls remove && sh ./scripts/teardown.sh"
  },
  "keywords": [
    "pdf",
    "pdf-generator",
    "azure-functions",
    "azure",
    "serverless"
  ],
  "author": "Mikael Vesavuori",
  "license": "ISC",
  "dependencies": {
    "@azure/functions": "^1.2.2",
    "@azure/storage-blob": "^12.2.0-preview.1",
    "@pdf-lib/fontkit": "^1.0.0",
    "pdf-lib": "^1.9.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/types": "^7.10.4",
    "@types/node": "^14.0.22",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-loader": "^8.1.0",
    "prettier": "^2.0.5",
    "serverless-azure-functions": "^2.0.13",
    "serverless-webpack": "^5.3.2",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
